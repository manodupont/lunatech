{
  "name": "lunatech",
  "version": "1.0.0",
  "description": "This is the lunatech client application application.",
  "main": "index.js",
  "author": "Manuel Dupont <manodupont@hotmail.com>",
  "license": "ISC",
  "scripts": {
    "build": "babel-node tools/run build",
    "build:client": "NODE_ENV=development npm run build",
    "build:test": "NODE_ENV=development babel-watch --watch ./src tools/run build",
    "docker": "docker build -t lunatech/cart .",
    "test": "jest -u --verbose",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm test -- --watch",
    "start:client": "NODE_ENV=development webpack-dev-server --config ./webpack.config.js --content-base build/public",
    "start:server": "npm run db && node ./src/server/index.js",
    "docker:clean": "docker stop $(docker ps -aq) && docker rm $(docker ps -aq)",
    "db": "docker run --name my-mongo -d -p 27017:27017 mongo:latest",
    "start": "docker run --name lunatech-cart-app -p 8080:80 -p 3000:3000 lunatech/cart",
    "server": "nginx && node ./src/server/index.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "async": "^2.6.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "bluebird": "^3.5.3",
    "boolean": "^0.2.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "colors": "^1.3.2",
    "connected-react-router": "^4.5.0",
    "express": "^4.16.4",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.13",
    "node-fetch": "^2.3.0",
    "querystring": "^0.2.0",
    "react": "^16.4.1",
    "react-addons-update": "^15.6.2",
    "react-arcgis": "^3.3.3",
    "react-bootstrap": "^1.0.0-beta.3",
    "react-dom": "^16.6.0",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.5.3",
    "react-google-maps": "^9.4.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.23.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.5.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "style-loader": "0.19.0",
    "tracer": "^0.9.6"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.9.7",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.22.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "del": "^3.0.0",
    "dotenv": "^6.1.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.3",
    "jest": "20.0.4",
    "jest-fetch-mock": "^1.6.6",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "^4.9.3",
    "prettier": "1.13.7",
    "prop-types": "^15.6.2",
    "regenerator-runtime": "^0.12.1",
    "replace-in-file": "^3.4.2",
    "sass-loader": "^7.1.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.3",
    "webpack-auto-clean-build-plugin": "^1.0.4",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-manifest-plugin": "1.3.2",
    "webpack-obfuscator": "^0.18.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[./a-zA-Z0-9$_-]+\\.png$": "RelativeImageStub",
      "^.*\\.less": "<rootDir>/tools/lib/LESSStub.js"
    },
    "automock": false,
    "setupFiles": [
      "./tools/setupJest.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "verbose": true,
    "modulePathIgnorePatterns": [
      "rpmbuild"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/enzyme/",
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/core-js"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "syntax-class-properties",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
